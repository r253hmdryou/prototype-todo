{
  "name": "prototype-todo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "author": "r253hmdryou",
  "license": "MIT",
  "engines": {
    "node": "16.x"
  },
  "scripts": {
    "start": "pm2 start pm2.yaml",
    "start-in-container": "pm2-runtime pm2.yaml",
    "stop": "pm2 stop pm2.yaml",
    "restart": "pm2 reload pm2.yaml",
    "delete": "pm2 delete pm2.yaml",
    "list": "pm2 list",
    "log": "pm2 logs",
    "monitor": "pm2 monit",
    "prebuild": "rimraf dist",
    "build": "run-p build:*",
    "build:server": "tsc -p tsconfig.server.json",
    "build:db": "run-p build:db:*",
    "build:db:migrations": "tsc -p tsconfig.db.json",
    "build:db:config": "cpx src/db/sequelize/config/sequelize.js dist/db",
    "db:migrate": "dotenv sequelize-cli db:migrate",
    "db:unmigrate": "dotenv sequelize-cli db:migrate:undo",
    "check-update": "npm-check-updates",
    "verify": "run-s test lint",
    "pretest": "run-s prebuild build:db:migrations",
    "test": "dotenv -e .env.test tsc -p tsconfig.json --noEmit && dotenv -e .env.test jest",
    "lint": "eslint ./src --ext .ts",
    "generate:type": "swagger-typescript-api -p ./src/oas/v1.yaml -o ./src/server/types -n api.ts --no-client --route-types --responses",
    "generate:type:web": "swagger-typescript-api -p ./src/oas/v1.yaml -o ../web/src/api -n api.ts --route-types --responses"
  },
  "dependencies": {
    "argon2": "0.29.1",
    "compression": "1.7.4",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "dotenv": "16.0.1",
    "dotenv-cli": "6.0.0",
    "express": "4.18.1",
    "express-mysql-session": "2.1.8",
    "express-session": "1.17.3",
    "glob": "8.0.3",
    "helmet": "5.1.1",
    "i18next": "21.9.1",
    "mariadb": "3.0.1",
    "nodemailer": "6.7.8",
    "sequelize": "6.21.4",
    "sequelize-cli": "6.4.1",
    "uuid": "8.3.2",
    "value-schema": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@swc/core": "1.2.241",
    "@swc/jest": "0.2.22",
    "@types/argon2": "0.15.0",
    "@types/compression": "1.7.2",
    "@types/cookie-parser": "1.4.3",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/express-mysql-session": "2.1.3",
    "@types/express-session": "1.17.5",
    "@types/glob": "7.2.0",
    "@types/i18n": "0.13.4",
    "@types/jest": "28.1.7",
    "@types/node": "16.11.45",
    "@types/nodemailer": "6.4.5",
    "@types/supertest": "2.0.12",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.30.6",
    "@typescript-eslint/parser": "5.30.6",
    "cpx": "1.5.0",
    "eslint": "8.20.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-security": "1.5.0",
    "jest": "28.1.3",
    "jest-date-mock": "1.0.8",
    "npm-check-updates": "16.0.5",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "supertest": "6.2.4",
    "swagger-typescript-api": "10.0.1",
    "typescript": "4.7.4",
    "umzug": "3.2.1"
  }
}
